login: 	- post:/users -"sprema podatke za novog korisnika u bazu"	
	- get:/users - "šalje se username ili email za provjera za logina, vracaju se podaci o useru, pamti se u sessionu"
	- put:/users - "promjena podataka za korisnika"

report:	- post:/reports -"sprema novu prijavu u bazu"
	- get:/reports  -"ovisno o prijavljenom korisniku vraća ili sve korisnikove prijave ili sve prijave od ureda"
	- get:/reports/{id} - "salje sve podatke o jednom reporu"
	- put:/reports/{id} - "sluzi administratoru za promjenu statusa"

report location: - get:/reportsLocation - "vraca sve lokacije, zadnje statuse, naslove i kratke opise nerijesenih grupa prijava" {za veliku mapu"}

category: - get:/category - "vraca sva imena kategorija"

keywords: - get:/keyords - "vraca sve ključne riječi i njihove pridjeljenje kategorije {imena}"

filters: - get:/filtered/{categoryID, TS prijave od,TS prijave do, status, lokacija, radijus u km} - "vraca sve prijave koje zadovoljavaju uvjete i ovlasti korisnika iz trenutnog sessiona, za predane prazne vrijednosti vracaju se sve entorke koje zadovoljavaju ostale kriterije"



